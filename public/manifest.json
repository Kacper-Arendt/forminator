{
	"manifest_version": 3,
	"name": "Forminator",
	"description": "Forminator is a form filler extension",
	"version": "1.0.0",
	"action": {
		"default_popup": "index.html",
		"default_icon": {
			"16": "icon.png",
			"48": "icon.png",
			"128": "icon.png"
		}
	},
	"permissions": [
		"activeTab",
		"storage"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"theme_color": "#000",
	"author": "Kacper Arendt"
}

{
	"manifest_version": 3,
	"name": "FormTripper",
	"description": "FormTripper is a form filler extension made by Kacper Arendt.",
	"version": "1.0.0",
	"action": {
		"default_popup": "index.html",
		"default_icon": {
			"16": "icon.png",
			"48": "icon.png",
			"128": "icon.png"
		}
	},
	"permissions": ["activeTab", "storage", "scripting"],
	"host_permissions": ["<all_urls>"],
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"author": "Kacper Arendt"
}
